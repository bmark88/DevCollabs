{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "jsx": "preserve",
    "lib": ["dom", "es2015", "es2017"],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "preserveConstEnums": true
  },
  "include": ["src/**/*", "src/components/hooks/appData.tsx", "src/components/.Dropdown.tsx", "src/components/.send.tsx", "src/components/.users.tsx", "src/components/.topics.tsx", "src/components/.seo.tsx", "src/components/.groups.tsx", "src/components/.add.tsx", "src/components/.image.tsx"]
}